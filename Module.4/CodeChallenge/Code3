import java.util.Arrays;

public class Code3 {
    public static void main(String[] args) {
        int[] numbers = {1, 5, 2, 3, 7, 8, 4};
        int[] maxNumbers = findMaxProductNumbers(numbers);
        System.out.println("Two numbers with maximum product: " + Arrays.toString(maxNumbers));
    }

    public static int[] findMaxProductNumbers(int[] numbers) {
        if (numbers.length < 2) {
            throw new IllegalArgumentException("Array should contain at least two numbers");
        }

        Arrays.sort(numbers);

        int max1 = numbers[numbers.length - 1];
        int max2 = numbers[numbers.length - 2];

        return new int[]{max1, max2};
    }
}
